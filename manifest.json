{
  "manifest_version": 3,
  "name": "MatchFetch 23andme Edition",
  "version": "1.0.3",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "description": "Extract and save your 23andMe DNA match data, including ancestry compositions and detailed match information for research and analysis",
  "permissions": [
    "cookies"
  ],
  "host_permissions": [
    "*://*.23andme.com/*"
  ],
  "action": {
    "default_title": "MatchFetch 23andme Edition"
  },
  "background": {
    "service_worker": "background.js"
  }
}
